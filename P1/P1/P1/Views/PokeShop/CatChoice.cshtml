
@{
    ViewData["Title"] = "CatChoice";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>CatChoice</h1>

<div>
    <h1>This is what we have in that catagory</h1>

    <form asp-action="Choice">

        <label for="item">Our Items</label>
        <select name="item" id="item">
            @foreach (P1Database.Product obj in Model)
            {

                <option id="item" value="@obj.ProductName">@obj.ProductName</option>
            }
        </select>
        <input type="submit" value="submit">
    </form>


</div>
